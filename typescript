vnetworkInstall () {[0m[7;5H[1m[36m#############################
[0m    [1m[36m# Check existing network
[0m    [1m[36m#############################[0m[11;5H[1m[36mNET_NAME[0m=[1m[34m$1[0m
    [1m[36mAPP_SUBNET[0m=[1m[34m$2[0m[14;5H[1m[36m# check if network with same name already exixts[0m
    [1m[36mNET_CHECK[0m=[1m[34m$([0m[1m[31msudo docker network inspect [0m[1m[34m$NET_NAME[0m[1m[31m --format {{.Name}}[0m[1m[34m)[0m[17;5H[1m[36m# if same network already exists: master -> delete / other -> join [0m
    [1m[33mif [[0m [1m[34m$NET_CHECK[0m [1m[33m!=[0m [1m[33m""[0m [1m[33m&&[0m [1m[34m$NET_CHECK[0m [1m[33m=[0m [1m[34m$NET_ACT[0m [1m[33m];[0m [1m[33mthen[20;8Hecho[0m[1m[35m [0m[1m[33m"[0m[1m[35m#### NET_NAME = [0m[1m[34m$NET_NAME[0m[1m[33m"[32D
echo[0m[1m[35m [0m[1m[33m"[0m[1m[35m#### NET_CHECK = [0m[1m[34m$NET_CHECK[0m[1m[33m"[0m[23;8H[1m[36mDEFAULT[0m=[1m[34m$DEP_VNET_RE[0m[20D
[1m[33mecho[0m[1m[35m [0m[1m[34m$CONSOLE_HL[0m[16D
[1m[33mread[0m [1m[33m-e[0m [1m[33m-p[0m [1m[33m"[0m[1m[35m:::: Confirm renew network '[0m[1m[34m$NET_NAME[0m[1m[35m' [[0m[1m[34m$DEP_YES[0m[1m[35m] or [[0m[1m[34m$DEP_QUIT[0m[1m[35m] to quit (Defaultt: [0m[1m[34m$DEP_VNET_RE[0m[1m[33m"[0m: PROCEED[17D
[1m[36mPROCEED[0m=[1m[33m"[0m[1m[34m${PROCEED[0m[1m[33m:-[0m[1m[34m${DEFAULT}}[0m[1m[33m"[29;8Hif [[0m [1m[33m"[0m[1m[34m${PROCEED}[0m[1m[33m"[0m [1m[33m==[0m [1m[33m"[0m[1m[34m$DEP_YES[0m[1m[33m"[0m [1m[33m][0m [1m[33m;[0m [1m[33mthen[0m[36D
